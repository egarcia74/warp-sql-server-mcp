{
  "name": "warp-sql-server-mcp",
  "version": "1.0.0",
  "description": "MCP server for connecting Warp to SQL Server",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,mjs,cjs,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,mjs,cjs,json,md}\"",
    "markdown:lint": "markdownlint-cli2 \"**/*.md\"",
    "markdown:fix": "markdownlint-cli2 --fix \"**/*.md\"",
    "hooks:install": "./install-git-hooks.sh",
    "hooks:uninstall": "rm -f .git/hooks/*",
    "precommit": "npm run lint && npm run format:check && npm run markdown:lint && npm test",
    "prepush": "npm run precommit",
    "ci": "npm run lint && npm run format:check && npm run markdown:lint && npm run test:coverage",
    "audit:fix": "npm audit fix",
    "clean": "rm -rf node_modules coverage .vitest"
  },
  "keywords": [
    "mcp",
    "sql-server",
    "warp",
    "database"
  ],
  "author": "Eduardo Garcia",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.7.0",
    "dotenv": "^17.2.1",
    "mssql": "^10.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "markdownlint-cli2": "^0.18.1",
    "prettier": "^3.3.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
